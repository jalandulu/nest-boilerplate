{
  "name": "nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:local": "dotenv -e .env.local -- nest start --watch",
    "start:dev": "dotenv -e .env.development.local -- nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "dotenv -e .env.production.local -- node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate": "npx prisma generate",
    "migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "migrate:dev": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "migrate:production": "dotenv -e .env.production.local -- npx prisma migrate dev",
    "seed": "dotenv -e .env.local -- npx prisma db seed",
    "seed:dev": "dotenv -e .env.development.local -- npx prisma db seed",
    "deploy": "npx prisma generate && nest build && pm2 startOrRestart ecosystem.config.js"
  },
  "prisma": {
    "schema": "./src/infrastructures/database/prisma/schema",
    "seed": "ts-node ./src/infrastructures/database/prisma/seeders/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.622.0",
    "@aws-sdk/s3-request-presigner": "^3.622.0",
    "@fastify/compress": "^7.0.3",
    "@fastify/csrf-protection": "^6.4.1",
    "@fastify/helmet": "^11.1.1",
    "@fastify/multipart": "^8.3.0",
    "@fastify/static": "^7.0.4",
    "@nestjs-cls/transactional": "^2.4.1",
    "@nestjs-cls/transactional-adapter-prisma": "^1.2.3",
    "@nestjs-modules/mailer": "^2.0.2",
    "@nestjs/bull": "^10.2.0",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-fastify": "^10.3.10",
    "@nestjs/swagger": "^7.4.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.52.1",
    "@opentelemetry/instrumentation": "^0.52.1",
    "@opentelemetry/resources": "^1.25.1",
    "@opentelemetry/sdk-trace-base": "^1.25.1",
    "@opentelemetry/sdk-trace-node": "^1.25.1",
    "@opentelemetry/semantic-conventions": "^1.25.1",
    "@prisma/client": "5.18.0",
    "@prisma/instrumentation": "^5.17.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.10.3",
    "cache-manager": "^5.7.3",
    "cache-manager-redis-store": "^3.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "firebase-admin": "^12.3.0",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "luxon": "^3.5.0",
    "mime-types": "^2.1.35",
    "mqtt": "^5.10.0",
    "nestjs-cls": "^4.4.0",
    "nestjs-firebase": "^10.5.0",
    "nestjs-prisma": "^0.23.0",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "prisma-extension-pagination": "^0.7.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "s3-readstream": "^2.0.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.25",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.3.1",
    "@types/nodemailer": "^6.4.15",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.18.0",
    "prisma-dbml-generator": "^0.12.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
